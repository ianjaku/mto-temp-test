{
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleDirectories": [
        "node_modules",
        "src",
        "/usr/local/lib/node_modules",
        "/usr/lib/node_modules"
    ],
    "testMatch": [
        "**/tests/integration/**/*.[jt]s?(x)",
        "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
        "/tests/integration/contentservice/utils.ts"
    ]
}
