{
    "name": "@binders/binders-v3",
    "version": "0.0.2",
    "devDependencies": {
        "@types/archiver": "5.1.1",
        "@types/express": "^4.17.21",
        "@types/fs-extra": "^5.0.5",
        "@types/jest": "^29.5.14",
        "@types/jsdom": "^21.1.7",
        "@types/multer": "^2",
        "@types/node": "^22.15.3",
        "@types/node-fetch": "^2.6.12",
        "@types/ramda": "~0.28.23",
        "@types/redis": "^2.8.6",
        "@types/superagent": "^4.1.13",
        "chalk": "^4.1.2",
        "commander": "^2.20.3",
        "concurrently": "^6.5.1",
        "csv-parse": "^4.16.3",
        "esbuild": "~0.25.9",
        "eslint": "^7.25.0",
        "jest": "^30.0.4",
        "jest-environment-jsdom": "^30.0.4",
        "jest-junit": "^16.0.0",
        "jest-mock-extended": "^4.0.0",
        "jest-util": "^30.0.2",
        "nodemon": "^2.0.7",
        "ts-jest": "^29.4.0",
        "tsx": "^4.20.3",
        "typescript": "5.4.5"
    },
    "scripts": {
        "cleanup": "rm -rf dist",
        "setup": "yarn install --loglevel warn --ignore-engines",
        "dev": "nodemon --watch src -L -e ts,json --exec \"npm run compile\"",
        "dev:esbuild": "nodemon --watch src -L -e ts,json --exec \"npm run compile:esbuild\"",
        "compile": "tsc --skipLibCheck && node --inspect=0.0.0.0:8511 dist/src/main.js",
        "compile:esbuild": "node build.cjs && node --inspect=0.0.0.0:8511 dist/src/main.js",
        "transpile": "eslint --max-warnings=0 src && tsc --skipLibCheck",
        "transpile:esbuild": "node build.cjs",
        "lint": "eslint --max-warnings=0 --cache --ext .ts,.js,.jsx,.tsx src",
        "start": "NODE_ENV=production node dist/src/main.js binders-v3",
        "componenttest": "jest --config=tests/component/jest.json",
        "unittest": "jest --config=tests/unit/jest.json",
        "unittest:watch": "jest --watch --config=tests/unit/jest.json",
        "integrationtest": "jest --config=tests/integration/jest.json --runInBand"
    },
    "dependencies": {
        "@azure/openai": "^1.0.0-beta.12",
        "@binders/binders-service-common": "workspace:*",
        "@binders/client": "workspace:*",
        "@elastic/elasticsearch": "7.17.0",
        "@google/genai": "^1.16.0",
        "archiver": "^7.0.0",
        "class-autobind": "^0.1.4",
        "date-fns": "2.30.0",
        "exponential-backoff": "^3.1.2",
        "express": "^4.21.0",
        "fs-extra": "^7.0.1",
        "http-status-codes": "^1.4.0",
        "immutable": "^3.8.2",
        "jsdom": "^24.1.0",
        "marked": "^13.0.2",
        "moment": "^2.30.1",
        "mongoose": "^8.13.1",
        "multer": "^2.0.2",
        "pdf-merger-js": "^4.1.0",
        "prom-client": "^11.5.3",
        "ramda": "~0.28.0",
        "superagent": "^6.1.0",
        "tcomb": "^3.2.29",
        "wkhtmltopdf": "^0.4.0",
        "xmlbuilder": "^13.0.2"
    }
}
