{
    "name": "@binders/mtutil",
    "version": "1.0.0",
    "description": "A kit of utilities for manual.to development",
    "main": "dist/index.js",
    "scripts": {
        "setup": "yarn install --loglevel warn --ignore-engines",
        "compile": "tsup",
        "transpile": "eslint --max-warnings=0 src && tsc",
        "watch": "tsc -w",
        "clean": "rm -r dist",
        "build": "tsup",
        "build:tsc": "tsc --skipLibCheck",
        "build:tsup": "tsup",
        "cli": "node dist/cli.js",
        "mto": "bash ./bin/mto.sh",
        "dev:watch": "tsup --watch --onSuccess \"yarn workspace @binders/mtutil cli\"",
        "mtutil": "node dist/mtutil.js"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@binders/binders-service-common": "workspace:*",
        "@kubernetes/client-node": "^1.2.0",
        "ansi-escapes": "^7.0.0",
        "chalk": "^4.1.2",
        "commander": "^2.20.3",
        "date-fns": "2.30.0",
        "figures": "^6.1.0",
        "inquirer": "^12.0.0",
        "inquirer-autocomplete-prompt": "^2.0.1",
        "lodash.debounce": "^4.0.8",
        "prompts": "^2.4.2",
        "ramda": "~0.28.0",
        "strip-ansi": "^7.1.0",
        "ws": "^8.18.2"
    },
    "devDependencies": {
        "@binders/client": "workspace:*",
        "@types/fs-extra": "^11.0.3",
        "@types/node": "^22.15.3",
        "@types/node-fetch": "^2.6.12",
        "@types/prompts": "^2.4.9",
        "@types/ramda": "~0.28.23",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "child-process-promise": "^2.2.1",
        "eslint": "^7.25.0",
        "fs-extra": "^10.0.0",
        "inquirer": "^12.0.0",
        "inquirer-autocomplete-prompt": "^2.0.1",
        "inquirer-prompt-suggest": "^0.1.0",
        "inquirer-table-prompt": "^0.2.1",
        "node-fetch": "2.7.0",
        "nodemon": "^2.0.7",
        "ts-morph": "^20.0.0",
        "tslib": "^2.6.2",
        "tsup": "^8.5.0",
        "tsx": "^4.20.3",
        "typescript": "5.4.5"
    }
}
