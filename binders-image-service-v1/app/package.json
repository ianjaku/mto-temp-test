{
    "name": "@binders/image-v1",
    "version": "1.0.3",
    "author": "Tom De Coninck",
    "license": "UNLICENSED",
    "scripts": {
        "cleanup": "rm -rf dist",
        "setup": "yarn install --loglevel warn --ignore-engines",
        "dev": "nodemon --watch src -L -e ts,json --exec \"npm run compile\"",
        "dev:esbuild": "nodemon --watch src -L -e ts,json --exec \"npm run compile:esbuild\"",
        "compile": "tsc --skipLibCheck && node --trace-deprecation --inspect=0.0.0.0:8507 dist/src/api/main.js",
        "compile:esbuild": "node build.cjs && node --inspect=0.0.0.0:8507 dist/src/api/main.js",
        "transpile": "eslint --max-warnings=0 src && tsc --skipLibCheck",
        "transpile:esbuild": "node build.cjs",
        "lint": "eslint --max-warnings=0 --cache --ext .ts,.js,.jsx,.tsx src",
        "start": "NODE_ENV=production node dist/src/api/main.js",
        "componenttest": "jest --config=tests/component/jest.json --runInBand --verbose",
        "unittest": "jest --config=tests/unit/jest.json",
        "integrationtest": "jest --config=tests/integration/jest.json --runInBand"
    },
    "dependencies": {
        "@azure/storage-blob": "^12.14.0",
        "@binders/binders-service-common": "workspace:*",
        "@binders/client": "workspace:*",
        "@bitmovin/api-sdk": "^1.185.0",
        "aws-sdk": "^2.9.0",
        "busboy": "1.6.0",
        "child-process-promise": "^2.2.1",
        "commander": "^2.20.3",
        "date-fns": "2.30.0",
        "express": "^4.21.0",
        "file-type": "16.5.4",
        "fluent-ffmpeg": "^2.1.0",
        "http-status-codes": "^1.4.0",
        "md5-file": "^3.1.1",
        "moment": "^2.30.1",
        "mongoose": "^8.13.1",
        "node-fetch": "2.7.0",
        "ramda": "~0.28.0",
        "read-chunk": "^2.0.0",
        "sharp": "0.33.5",
        "shell-escape": "^0.2.0",
        "superagent": "^6.1.0"
    },
    "devDependencies": {
        "@types/busboy": "^1.5.3",
        "@types/express": "^4.17.21",
        "@types/fluent-ffmpeg": "^2.1.24",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.15.3",
        "@types/node-fetch": "^2.6.12",
        "@types/ramda": "~0.28.23",
        "@types/redis": "^2.8.6",
        "@types/validator": "^6.2.0",
        "chalk": "^4.1.2",
        "concurrently": "^6.5.1",
        "esbuild": "~0.25.9",
        "eslint": "^7.25.0",
        "jest": "^30.0.4",
        "jest-environment-jsdom": "^30.0.4",
        "jest-junit": "^16.0.0",
        "jest-mock-extended": "^4.0.0",
        "jest-util": "^30.0.2",
        "nodemon": "^2.0.7",
        "ts-jest": "^29.4.0",
        "tsx": "^4.20.3",
        "typescript": "5.4.5"
    }
}
